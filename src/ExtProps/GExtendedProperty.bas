Public Function gGetValidator(ByVal pValidatorPointer As Long) As PropertyValidator
Const ProcName As String = "gGetValidator"
On Error GoTo Err

If pValidatorPointer = 0 Then Exit Function
If pValidatorPointer <> mValidatorPointer Then
    RedirectMethod ObjPtr(mValidator), 7, pValidatorPointer
    mValidatorPointer = pValidatorPointer
End If
Set gGetValidator = mValidator

Exit Function

Err:
gHandleUnexpectedError ProcName, ModuleName
End Function

